            <TextBlock 
                Text="专业EQ"
                HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="60,390,0,0" FontWeight="Bold" FontSize="20" Foreground="#44000000"
                />
        </Grid>
        <Grid Margin="200,50,0,0" Background="#F1F1F1">
            <StackPanel Orientation="Horizontal" Margin="50,140,50,120">
                <TextBlock 
                    Text="30db"
                    HorizontalAlignment="Left" VerticalAlignment="Top"
                    Margin="13,0,0,0" FontSize="13" Foreground="Black"
                    />
                <TextBlock 
