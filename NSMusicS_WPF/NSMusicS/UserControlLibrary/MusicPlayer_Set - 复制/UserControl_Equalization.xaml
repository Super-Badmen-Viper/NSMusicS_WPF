                VerticalAlignment="Top" Margin="70,200,0,0"  
                >
                <svgc:SvgViewbox Source="pack://application:,,,/UI_0_Library/调节 设置均衡器.svg"/>
            </TextBlock>
            <TextBlock 
                Text="普通EQ"
                HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="60,260,0,0" FontWeight="Bold" FontSize="20" Foreground="#44000000"
                />
            
            <TextBlock 
                HorizontalAlignment="Left" Height="60" Width="60"
                VerticalAlignment="Top" Margin="70,330,0,0"  
