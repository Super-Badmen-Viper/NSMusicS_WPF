                    HorizontalAlignment="Left" VerticalAlignment="Top"
                    Margin="6,0,0,0" FontSize="13" Foreground="Black"
                    />
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="50,160,50,160" Background="Black">
                <Slider Orientation="Vertical" Margin="20,0,0,0" Minimum="{Binding MinimumGain}" Maximum="{Binding MaximumGain}" Value="{Binding Band1, Mode=TwoWay}"></Slider>
