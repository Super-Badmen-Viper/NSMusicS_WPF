                    <svgc:SvgViewbox Source="pack://application:,,,/UI_0_Library/音效_.svg"/>
            </TextBlock>
            <TextBlock 
                HorizontalAlignment="Right" Height="30" Width="30"
                VerticalAlignment="Center" Margin="0,0,30,0"  
                >
                    <svgc:SvgViewbox Source="pack://application:,,,/UI_0_Library/关闭_.svg"/>
            </TextBlock>
            <TextBlock 
                Text="MoZhi墨智 EQ均衡器 （更新中）"
                HorizontalAlignment="Left" VerticalAlignment="Center"
                Margin="80,0,0,0" FontWeight="Bold" FontSize="26" Foreground="Black"
                />
        </Grid>
        <Grid Width="200" HorizontalAlignment="Left" Margin="0,50,0,0" Background="White">
            <TextBlock 
                HorizontalAlignment="Left" Height="60" Width="60"
                VerticalAlignment="Top" Margin="70,70,0,0"  
                >
                <svgc:SvgViewbox Source="pack://application:,,,/UI_0_Library/调节 设置均衡器.svg"/>
            </TextBlock>
            <TextBlock 
                Text="简易EQ"
                HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="60,130,0,0" FontWeight="Bold" FontSize="20" Foreground="Black"
